p#notice = notice

p
  strong Repo:
  = @tag.repo
p
  strong Name:
  = @tag.name
p
  strong Git url:
  = @tag.git_url
p
  strong Git commit:
  = @tag.git_commit
p
  strong Dockerfile dir:
  = @tag.dockerfile_dir
div
  strong Dockerfile url:
  div = link_to @tag.dockerfile_url, @tag.dockerfile_url, target: '_blank'
p
  strong Dockerfile downloaded at:
  = @tag.dockerfile_downloaded_at

div
  strong Buildable url:
  div = @tag.buildable_url

div
  strong Dockerfile content:
  pre = @tag.dockerfile_content

= link_to 'Edit', edit_tag_path(@tag)
'|
= link_to 'Back', tags_path

css:
  pre {
    background: #eee;
  }
